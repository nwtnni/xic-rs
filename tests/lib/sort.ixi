use vector

// Requires:
//
// class T {
//   compare(other: T): int
// }
template bubbleSort<T>(input: Vector::<T>) {
    swapped: bool = true

    while swapped {
        swapped = false

        i: int = 0
        while i + 1 < input.size() {
            if input.get(i).compare(input.get(i + 1)) > 0 {
                input.swap(i, i + 1)
                swapped = true
            }

            i = i + 1
        }
    }
}
