use assert
use conv
use io
use math
use string
use vector

INPUT: int[] = "\n\
\n\
\n\
\n\
"

main(args: int[][]) {

    lines: Vector::<String> = new_string_from_array(INPUT).split('\n')

    i: int = 0

    while i < lines.size() {

        x: int = 0
        y: int = 0

        j: int = 0

        while j < lines.get(i).size() {
            if lines.get(i).get(j) == 'U' {
                y = clamp(y - 1, 0, 2)
            } else if lines.get(i).get(j) == 'D' {
                y = clamp(y + 1, 0, 2)
            } else if lines.get(i).get(j) == 'L' {
                x = clamp(x - 1, 0, 2)
            } else if lines.get(i).get(j) == 'R' {
                x = clamp(x + 1, 0, 2)
            } else {
                assert(false)
            }

            j = j + 1
        }

        print(unparseInt(y * 3 + x + 1))
        i = i + 1
    }

    println("")
}
