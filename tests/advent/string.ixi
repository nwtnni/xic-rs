use vector

final class String {
    size(): int

    get(index: int): int

    get_array(): int[]

    push(character: int)

    pop(): int

    starts_with(substring: String): bool

    contains(substring: String): bool

    split(character: int): Vector::<String>

    split_string(substring: String): Vector::<String>

    slice(low: int, high: int): String

    slice_array(low: int, high: int): int[]
}

new_string(): String

new_string_from_array(array: int[]): String

new_string_from_vector(vector: Vector::<int>): String
